{
  "name": "habitmax-backend",
  "version": "1.0.0",
  "description": "HabitMax Bot Backend - Чат-бот для привычек и напоминаний о лекарствах для платформы Max",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:bot": "nodemon src/index.js --mode=polling",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "node src/migrations/run.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "chatbot",
    "max",
    "habits",
    "reminders",
    "health",
    "russia"
  ],
  "author": "HabitMax Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "bullmq": "^4.15.0",
    "ioredis": "^5.3.2",
    "axios": "^1.6.2",
    "bottleneck": "^2.19.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "moment-timezone": "^0.5.43",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "minio": "^7.1.3",
    "csv-writer": "^1.6.0",
    "node-cron": "^3.0.3",
    "express-rate-limit": "^7.1.5",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0"
  }
